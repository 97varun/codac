{
	"name": "codac",
	"displayName": "codac",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:c"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "codac",
					"title": "Codac",
					"icon": "media/dep.svg"
			  	}
			]
		},
		"views": {
			"codac": [
				{
					"id": "suggestions",
					"name": "Suggestions"
				},
				{
					"id": "otherinfo",
					"name": "other info"
				}
			]
		},
		"commands": [
			{
				"command": "extension.listen",
				"title": "Listen"
			},
			{
				"command": "extension.stop",
				"title": "Stop"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"child_process": "^1.0.2",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {}
}
